{
  "name": "ux-kitchen-sink",
  "version": "0.0.0",
  "scripts": {
    "themes": "babel-node src/themes",
    "doc": "npm-run-all doc:*",
    "doc:js": "babel src/doc/static/js -d dist/doc/js",
    "doc:img": "copy 'src/doc/static/img/*.*' dist/doc/img",
    "doc:css": "node-sass src/doc/static/css/main.scss dist/doc/css/main.css",
    "doc:build": "babel-node src/doc",
    "watch": "parallel watch:themes watch:doc",
    "watch:themes": "watch 'npm run themes' src/themes",
    "watch:doc": "watch 'npm run doc' src/doc",
    "clean": "cleandir dist",
    "build": "npm-run-all clean themes doc",
    "dev": "parallel server watch",
    "server": "npm run build && browser-sync start -s dist -f dist"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.18.13",
    "clean-dir": "^1.0.3",
    "copy": "^0.3.1",
    "front-matter": "^2.2.0",
    "fs-extra": "^4.0.2",
    "markdown-it": "^8.4.0",
    "node-sass": "^4.5.3",
    "npm-parallel": "^2.0.1",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.13",
    "postcss-cli": "^4.1.1",
    "pug": "^2.0.0-rc.4",
    "watch": "^1.0.2"
  }
}
